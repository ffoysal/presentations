[{
  "name": "meetup",
  "image": "${container_image_url}",
  "cpu": 512,
  "memory": 512,
  "essential": true,
  "environment": [
    {
      "name": "SQS_URL",
      "value": "${sqs_url}"
    },
    {
      "name": "AWS_REGION",
      "value": "${region}"
    }
  ],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "${log_group_name}",
      "awslogs-region": "${region}",
      "awslogs-stream-prefix": "rebel"
    }
  }
}]